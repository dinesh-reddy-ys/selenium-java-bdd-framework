<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="ParallelSuite" verbose="1" parallel="methods" thread-count="2">
    <test name="ChromeTests">
        <parameter name="browser" value="safari"/>
        <classes>
            <class name="testRunners.ParallelTestRunner"/>
        </classes>
    </test>
    <test name="Rerun failed Tests" >
        <parameter name="browser" value="safari"/>
        <classes>
            <class name="testRunners.FailedTestRunner"/>
        </classes>
    </test>
</suite>
